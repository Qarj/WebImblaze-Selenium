<testcases repeat="1">

<case
    section="Locators for Testers Example - Caterer"
    id="100"
    description1="Fill out Caterer search form"
    method="selenium"
    command1='$driver->get("https://www.caterer.com")'
    command2="_keys_to_element_after('What','Chef')"
    command3="_keys_to_element_after('Where','London')"
    command4="_keys_to_element_after('Where','5 miles','SELECT')"
    verifytext="get_current_url,get_body_text"
    verifypositive="the best jobs"
    checkpoint="true"
/>

<case
    id="110"
    description1="Submit Search form"
    method="selenium"
    command1='_click("Search")'
    verifytext="get_current_url,get_body_text"
    verifypositive="Chef jobs in London"
/>

<case
    id="120"
    description1="Select West End facet"
    method="selenium"
    command1='_click("West End")'
    verifytext="get_current_url,get_body_text"
    verifypositive="Chef jobs in West End"
/>

</testcases>