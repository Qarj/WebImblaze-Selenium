<testcases repeat="1">

<case
    section="Locators for Testers Example - Panera Bread"
    id="100"
    description1="Fill out search form"
    method="selenium"
    command1='$driver->get("https://www.panerabread.com")'
    command2='_keys_to_element("City/State","Las Vegas")'
    verifytext="get_current_url,get_body_text"
    verifypositive="MyPanera"
/>

<case
    id="110"
    description1="Submit search"
    method="selenium"
    command1='_click("Find")'
    command2='_wait_visible("Hughes Center")'
    verifytext="get_current_url,get_body_text"
    verifypositive1="Panera Bread Locations"
    verifypositive2="Las Vegas"
/>

</testcases>