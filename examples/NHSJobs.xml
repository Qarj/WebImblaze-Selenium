<testcases repeat="1">

<case
    section="Locators for Testers Example - NHS jobs"
    id="100"
    description1="Fill out NHS Jobs search form"
    method="selenium"
    command1='$driver->get("https://www.jobs.nhs.uk")'
    command2="_keys_to_element_after('Keywords','Surgeon')"
    command3="_keys_to_element_after('Location','London')"
    verifytext="get_current_url,get_body_text"
    verifypositive="apply for NHS jobs online"
/>

<case
    id="110"
    description1="Submit Search form"
    method="selenium"
    command1='_click("Search")'
    verifytext="get_current_url,get_body_text"
    verifypositive="search results"
/>

<case
    id="120"
    description1="Sort by salary - greatest first"
    method="selenium"
    command1="_keys_to_element_after('Sort by','Salary (greatest first)','SELECT')"
    verifytext="get_current_url,get_body_text"
    verifypositive="greatest first"
/>

</testcases>